class CustomerStack:
  def __init__(self,max_value):
    self.stack=[]
    self.max_value=max_value
  def push(self,item):
    if len(self.stack)<(self.max_value):
      self.stack.append(item)
      print(item)
    else:
      print("stack overflow")
stack=CustomerStack(3)
stack.push(2)
stack.push(3)
stack.push(4)
stack.push(5)
stack.push(6)
    
